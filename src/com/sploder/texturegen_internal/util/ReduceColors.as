	public class ReduceColors 
	{
		public static var CHARS:String = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/";
		
		public static const DITHER_CHECKER:int = 1;
		public static const DITHER_HORIZLINE:int = 2;
		public static const DITHER_VERTLINE:int = 3;
		public static const DITHER_SCREEN:int = 4;
		public static const DITHER_CHECKER_DOUBLE:int = 5;
		public static const DITHER_HORIZLINE_DOUBLE:int = 6;
		public static const DITHER_VERTLINE_DOUBLE:int = 7;
		public static const DITHER_SCREEN_DOUBLE:int = 8;
		public static const DITHER_NOISE:int = 9;
		public static const DITHER_BITMAP_01:int = 10;
		public static const DITHER_BITMAP_02:int = 11;
		public static const DITHER_BITMAP_03:int = 12;
		public static const DITHER_BITMAP_04:int = 13;
		public static const DITHER_BITMAP_05:int = 14;
		public static const DITHER_BITMAP_06:int = 15;
		public static const DITHER_BITMAP_07:int = 16;
		public static const DITHER_BITMAP_08:int = 17;
		public static const DITHER_BITMAP_09:int = 18;
		public static const DITHER_BITMAP_10:int = 19;
		public static const DITHER_BITMAP_11:int = 20;
		public static const DITHER_BITMAP_12:int = 21;
		public static const DITHER_BITMAP_13:int = 22;
		public static const DITHER_BITMAP_14:int = 23;
		public static const DITHER_BITMAP_15:int = 24;
		public static const DITHER_BITMAP_16:int = 25;
		public static const DITHER_BITMAP_17:int = 26;
		public static const DITHER_BITMAP_18:int = 27;
		public static const DITHER_BITMAP_19:int = 28;
		public static const DITHER_BITMAP_20:int = 29;
		public static const DITHER_ORDERED:int = 30;
		public static const DITHER_STUCKI:int = 31;
		public static const DITHER_FLOYD_STEINBERG:int = 32;
		public static const DITHER_FLOYD_STEINBERG_FALSE:int = 33;
		
		
		private static var bayerThresholdMap:Array = [
		  [  15, 135,  45, 165 ],
		  [ 195,  75, 225, 105 ],
		  [  60, 180,  30, 150 ],
		  [ 240, 120, 210,  90 ]
		];
		
		public function ReduceColors() { }
		
		public static function reduceColors(img:BitmapData, number:int = 16, ditherMode:int = 0):void
		{
			if (ditherMode > DITHER_ORDERED)
			{
				switch (ditherMode)
				{
					case DITHER_STUCKI:
						ImageDithering.dither(img, ImageDitheringType.STUCKI, Math.floor(number / 2), false);
						
					case DITHER_FLOYD_STEINBERG:
						ImageDithering.dither(img, ImageDitheringType.FLOYD_STEINBERG, Math.floor(number / 2), false);
						
					case DITHER_FLOYD_STEINBERG_FALSE:
						ImageDithering.dither(img, ImageDitheringType.FALSE_FLOYD_STEINBERG, Math.floor(number / 2), false);
						
				}
				
				return;
			}
			
			var i:int = 0;
			var j:int = 0;
			var x:int = 0;
			var y:int = 0;
			
			var val:int = 0;
			
			var total:int = 255;
			number -= 2;
			if( number <= 0 ) number = 1;
			if( number >= 255) number = 254;
			
			var step:Number = total / number;
			var offset:Number = ( total - ( total / ( number + 1 ) ) ) / total;
			var values:Array = [];
			
			for (i = 0; i < total; i++)
			{
				if( i >= ( j * step * offset ) )
				{
					j++;
				}
				values.push( Math.floor( ( Math.ceil( j*step )-step ) ) );
			}
			
			var a:int;
			var r:int;
			var g:int;
			var b:int;
			var c:int;
			var d:Boolean = false;
			
			var iw:int = img.width;
			var ih:int = img.height;
			var dither_map:BitmapData = null;
			
			img.lock();
			
			if (ditherMode >= DITHER_BITMAP_01 && ditherMode <= DITHER_BITMAP_20)
			{
				var map_idx:int = ditherMode - DITHER_BITMAP_01;
				
				if (ditherMaps[map_idx] == null)
				{
					var s:String = ditherMapData[map_idx];
					var size:int = Math.floor(Math.sqrt(s.length));
					
					dither_map = new BitmapData(size, size, false);
					
					for (i = 0; i < s.length; i++)
					{
						dither_map.setPixel(i % size, Math.floor(i / size), s.charAt(i) == "0" ? 0 : 0xffffff);
					}
				} 
				else 
				{
					dither_map = ditherMaps[map_idx];
				}
				
				if (dither_map == null)
				{
					ditherMode = 0;
				}
			}

			// COLORS WITH ALPHA NOT AFFECTED
			for (i = 0; i < iw; i++)
			{
				for (j = 0; j < ih; j++)
				{
				
					if (img.transparent)
					{
						val = img.getPixel32(i, j);
						a = val >>> 24;
						val = img.getPixel(i, j);
					} else {
						val = img.getPixel(i, j);
					}
				
					
					
					ColorComponents.setFromColor(val);
					
					
					if (ditherMode >= DITHER_BITMAP_01 && ditherMode <= DITHER_BITMAP_20)
					{
						d = dither_map.getPixel(Math.floor(i % dither_map.width), Math.floor(j % dither_map.height)) < 0x999999;
					} 
					else if (ditherMode > 0)
					{
						switch (ditherMode)
						{
							case DITHER_CHECKER:
								d = ((i + j) % 2 == 0);
								
							case DITHER_HORIZLINE:
								d = (j % 2 == 0);
								
							case DITHER_SCREEN:
								d = ((i * j) % 2 == 0);
								
							case DITHER_VERTLINE:
								d = (i % 2 == 0);
								
							case DITHER_CHECKER_DOUBLE:
								d = ((Math.floor(i / 2) + Math.floor(j / 2)) % 2 == 0);
								
							case DITHER_HORIZLINE_DOUBLE:
								d = (Math.floor(j / 2) % 2 == 0);
								
							case DITHER_SCREEN_DOUBLE:
								d = ((Math.floor(i / 2) * Math.floor(j / 2)) % 2 == 0);
								
							case DITHER_VERTLINE_DOUBLE:
								d = (Math.floor(i / 2) % 2 == 0);
								
							case DITHER_NOISE:
								d = (Math.random() > 0.5);
								
							case DITHER_ORDERED:
								d = (Math.floor( (ColorTools.getBrightness(val) + bayerThresholdMap[i % 4][j % 4]) / 2 ) < 128);
								
						}
						
					}
					
					if (d) 
					{
						ColorComponents.red = Math.floor(Math.max(0, ColorComponents.red - 18));
						ColorComponents.green = Math.floor(Math.max(0, ColorComponents.green - 18));
						ColorComponents.blue = Math.floor(Math.max(0, ColorComponents.blue - 18));
					}
					
					if (img.transparent && a < 196) 
					{
						a = (a > 128) ? 0xFF : 0;
						if (i < 10 || j < 10)
						{
							r = g = b = 0xff - Math.floor(Math.min(i, j));
						} else {
							r = g = b = 0;
						}
					} else {
						r = values[ ( ColorComponents.red & 0xFF ) - 1 ];
						g = values[ ( ColorComponents.green & 0xFF ) - 1 ];
						b = values[ ( ColorComponents.blue & 0xFF ) - 1 ];
					}
					
					if (img.transparent) img.setPixel32(i, j, a << 24 | r << 16 | g << 8 | b);
					else img.setPixel( i, j, r << 16 | g << 8 | b);
				
				}
			}
			
			img.unlock();
			
		}
		
		//SHORTCUTS
		
		public static function toCGA( bmpd:BitmapData, ditherMode:int = 0):void
		{
			 reduceColors( bmpd, 0, ditherMode);
		}	
		
		public static function toEGA( bmpd:BitmapData, ditherMode:int = 0):void
		{
			 reduceColors( bmpd, 4, ditherMode);
		}
		
		public static function toHAM( bmpd:BitmapData, ditherMode:int = 0):void
		{
			 reduceColors( bmpd, 6, ditherMode);
		}	
		
		public static function toVGA( bmpd:BitmapData, ditherMode:int = 0):void
		{
			 reduceColors( bmpd, 8, ditherMode);
		}
		
		public static function toSVGA( bmpd:BitmapData, ditherMode:int = 0):void
		{
			 reduceColors( bmpd, 16, ditherMode);
		}
		
